#!/usr/bin/env bash
# This script displays "Best School" 10 times, but on the 9th iteration, it says "Hi" on a new line.
# Initialize a counter
count=0
# Use a while loop to iterate 10 times
while [ $count -lt 10 ]; do
    if [ $count -eq 8 ]; then
        echo "Hi"
    else
        echo "Best School"
    fi
    # Increment the counter
    count=$((count + 1))
done
